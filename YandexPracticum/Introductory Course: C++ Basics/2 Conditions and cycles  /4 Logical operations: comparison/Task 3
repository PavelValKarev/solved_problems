#include <iostream>

using namespace std;

int main()
{
    double i1, i2, i3;
    cin >> i1 >> i2;
    
    (i1 == i2) && (cout << "The orange is divided into two equal parts"s);

    if (i1 < i2) {
        i3 = i2 - i1;
        cout << "Cut off "s  << i3 << " from the second part"s << endl;
    }
    else if(i1>i2) {
        i3 = i1 - i2;
        cout << "Cut off "s << i3 << " from the first part"s << endl;
    }    
}
